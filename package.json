{
  "name": "web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.6.0",
  "engines": {
    "node": "14.x || 16.x"
  },
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite",
    "lint": "eslint --ext .ts,.js,.json,.vue .",
    "prepare": "husky install scripts/husky",
    "preview": "vite preview",
    "test": "vitest run --globals",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^1.6.0",
    "@vueuse/core": "^9.6.0",
    "@vueuse/head": "^0.7.13",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.25.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@fontsource/nunito": "^4.5.11",
    "@fontsource/work-sans": "^4.5.12",
    "@iconify/vue": "^3.2.1",
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@rollup/pluginutils": "^4.2.1",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/server-renderer": "^3.2.45",
    "@vue/test-utils": "^2.2.4",
    "@yankeeinlondon/meta-builder": "^1.0.6",
    "eslint": "^8.28.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "prismjs": "^1.29.0",
    "rollup": "^2.79.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^2.9.15",
    "vite-plugin-md": "^0.20.4",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-pwa": "^0.12.8",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-windicss": "^1.8.8",
    "vite-ssg": "^0.20.2",
    "vite-ssg-sitemap": "^0.3.2",
    "vitest": "^0.21.1",
    "vue-tsc": "^0.39.5",
    "windicss": "^3.5.6",
    "workbox-window": "^6.5.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,ts,vue}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@antfu/vue"
    ],
    "globals": {
      "vi": true,
      "vitest": true
    },
    "ignorePatterns": [
      "**/*.test.ts",
      "**/*.d.ts"
    ],
    "parserOptions": {
      "tsconfigRootDir": ".",
      "project": "./tsconfig.json"
    },
    "rules": {
      "vue/component-name-in-template-casing": [
        "warn",
        "kebab-case"
      ],
      "vue/component-definition-name-casing": [
        "error",
        "kebab-case"
      ],
      "vue/html-self-closing": "off"
    }
  }
}
