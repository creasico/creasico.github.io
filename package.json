{
  "name": "web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": "16.x || 18.x"
  },
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite",
    "lint": "eslint --ext .ts,.js,.json,.vue .",
    "prepare": "husky install scripts/husky",
    "preview": "vite preview",
    "test": "vitest run --globals",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^2.0.0",
    "@vueuse/core": "^10.2.0",
    "@vueuse/head": "^1.1.26",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.39.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@fontsource/nunito": "^5.0.3",
    "@fontsource/work-sans": "^5.0.3",
    "@iconify/vue": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@rollup/pluginutils": "^5.0.2",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^20.3.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vue/test-utils": "^2.4.0",
    "@yankeeinlondon/meta-builder": "^1.4.0",
    "eslint": "^8.43.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "prismjs": "^1.29.0",
    "rollup": "^3.25.1",
    "typescript": "^5.1.3",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-windicss": "^1.9.0",
    "vite-ssg": "^0.22.2",
    "vite-ssg-sitemap": "^0.5.1",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.8.1",
    "windicss": "^3.5.6",
    "workbox-window": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,ts,vue}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@antfu/vue"
    ],
    "globals": {
      "vi": true,
      "vitest": true
    },
    "ignorePatterns": [
      "**/*.test.ts",
      "**/*.d.ts"
    ],
    "parserOptions": {
      "tsconfigRootDir": ".",
      "extraFileExtensions": [
        ".vue"
      ],
      "project": "./tsconfig.json"
    },
    "rules": {
      "vue/component-name-in-template-casing": [
        "warn",
        "kebab-case"
      ],
      "vue/component-definition-name-casing": [
        "error",
        "kebab-case"
      ],
      "vue/html-self-closing": "off"
    }
  }
}
