{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.6.0",
  "engines": {
    "node": "14.x || 16.x"
  },
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite",
    "lint": "eslint --ext .ts,.js,.json,.vue .",
    "prepare": "husky install scripts/husky",
    "preview": "vite preview",
    "test": "vitest run --globals",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^1.6.0",
    "@vueuse/core": "^9.1.0",
    "@vueuse/head": "^0.7.9",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.25.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@fontsource/nunito": "^4.5.9",
    "@fontsource/work-sans": "^4.5.10",
    "@iconify/vue": "^3.2.1",
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@rollup/pluginutils": "^4.2.1",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "@vue/test-utils": "^2.0.2",
    "@yankeeinlondon/meta-builder": "^1.0.6",
    "eslint": "^8.21.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.4",
    "prismjs": "^1.28.0",
    "rollup": "^2.77.2",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.0",
    "unplugin-vue-components": "^0.22.0",
    "vite": "^2.9.14",
    "vite-plugin-md": "^0.20.2",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-pwa": "^0.12.3",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-windicss": "^1.8.7",
    "vite-ssg": "^0.20.2",
    "vite-ssg-sitemap": "^0.3.1",
    "vitest": "^0.21.0",
    "vue-tsc": "^0.39.4",
    "windicss": "^3.5.6",
    "workbox-window": "^6.5.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,ts,vue}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@antfu/vue"
    ],
    "globals": {
      "vi": true,
      "vitest": true
    },
    "ignorePatterns": [
      "**/*.test.ts",
      "**/*.d.ts"
    ],
    "rules": {
      "vue/component-name-in-template-casing": [
        "warn",
        "kebab-case"
      ],
      "vue/component-definition-name-casing": [
        "error",
        "kebab-case"
      ],
      "vue/html-self-closing": "off"
    }
  }
}
