{
  "name": "web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": "16.x || 18.x"
  },
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite",
    "lint": "eslint --ext .ts,.js,.json,.vue .",
    "prepare": "husky install scripts/husky",
    "preview": "vite preview",
    "test": "vitest run --globals",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^2.0.0",
    "@vueuse/core": "^10.2.1",
    "@vueuse/head": "^1.1.26",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@creasico/eslint-config": "^0.0.1",
    "@fontsource/nunito": "^5.0.5",
    "@fontsource/work-sans": "^5.0.5",
    "@iconify/vue": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@rollup/pluginutils": "^5.0.2",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^20.4.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "@yankeeinlondon/meta-builder": "^1.4.0",
    "eslint": "^8.45.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^10.5.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "prismjs": "^1.29.0",
    "rollup": "^3.26.3",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.6",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-windicss": "^1.9.0",
    "vite-ssg": "^0.23.1",
    "vite-ssg-sitemap": "^0.5.1",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.6",
    "windicss": "^3.5.6",
    "workbox-window": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,ts,vue}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@creasico/eslint-config/vue"
    ],
    "globals": {
      "vi": true,
      "vitest": true
    },
    "ignorePatterns": [
      "**/*.test.ts"
    ],
    "parserOptions": {
      "tsconfigRootDir": ".",
      "project": "./tsconfig.json"
    }
  }
}
